    <fieldUpdates>
        <fullName>Additional_Scorecard_Completed</fullName>
        <field>Completed__c</field>
        <literalValue>1</literalValue>
        <name>Additional Scorecard Completed</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <rules>
        <fullName>Action Items Complete%3A Email</fullName>
        <actions>
            <name>Data_Scorecard_Action_Items_Completed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2 AND (3 OR 4 OR 5)</booleanFilter>
        <criteriaItems>
            <field>MAP_Scorecard__c.Action_Items_Outstanding__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>MAP_Scorecard__c.Status__c</field>
            <operation>equals</operation>
            <value>Completed</value>
        </criteriaItems>
        <criteriaItems>
            <field>MAP_Scorecard__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Case Management</value>
        </criteriaItems>
        <criteriaItems>
            <field>MAP_Scorecard__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>List Management</value>
        </criteriaItems>
        <criteriaItems>
            <field>MAP_Scorecard__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Account Management</value>
        </criteriaItems>
        <description>Notifies the Manager when Employee unchecks &apos;Action Items Outstanding&apos; checkbox</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Contact Call Scorecard Completed</fullName>
        <actions>
            <name>Contact_Call_Scorecard_Alert</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>MAP_Scorecard__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Contact Call Scorecard</value>
        </criteriaItems>
        <criteriaItems>
            <field>MAP_Scorecard__c.Status__c</field>
            <operation>equals</operation>
            <value>Completed</value>
        </criteriaItems>
        <description>Triggers email alert for Contact Call Scorecard.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Content Scorecard Completed%3A Email</fullName>
        <actions>
            <name>Content_Scorecard_Feedback_Email_Related_Users</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>MAP_Scorecard__c.Overall_Content_Score__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>MAP_Scorecard__c.Completed_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>MAP_Scorecard__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Content Quality Scorecard</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
