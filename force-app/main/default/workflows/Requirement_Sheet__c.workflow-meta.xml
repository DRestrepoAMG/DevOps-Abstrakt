    <rules>
        <fullName>Email Notification for New Website Audit</fullName>
        <actions>
            <name>Email_Notification_for_New_Website_Audit</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Requirement_Sheet__c.Submit__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Requirement_Sheet__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Website Audit</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Email Notification for Website Audit Approval</fullName>
        <actions>
            <name>Website_Audit_Approval_Notification</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Requirement_Sheet__c.Approved__c</field>
            <operation>equals</operation>
            <value>Yes,No</value>
        </criteriaItems>
        <criteriaItems>
            <field>Requirement_Sheet__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Website Audit</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Requirement_Sheet__c.CreatedDate</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
