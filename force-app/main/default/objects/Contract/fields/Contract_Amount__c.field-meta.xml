<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_Amount__c</fullName>
    <description>This formula multiplies the &quot;Total Amount by Billing Cycle&quot; by the &quot;Billing Period&quot;</description>
    <externalId>false</externalId>
    <formula>CASE( Billing_Period__c,&quot;Monthly&quot;,12,&quot;Quarterly&quot;,4,&quot;Semiannually&quot;,2,&quot;Annually&quot;,1,0)* Total_Amount__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This formula multiplies the &quot;Total Amount by Billing Cycle&quot; by the &quot;Billing Period&quot;</inlineHelpText>
    <label>Contract Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
