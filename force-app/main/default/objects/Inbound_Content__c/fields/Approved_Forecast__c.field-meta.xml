<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Approved_Forecast__c</fullName>
    <description>Forecast if a piece of content is going to be approved.</description>
    <externalId>false</externalId>
    <formula>IF( Revisions_Required__c =TRUE,TRUE,
IF(ISPICKVAL( Status__c ,&apos;Approved&apos;),TRUE,FALSE))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Approved Forecast</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
