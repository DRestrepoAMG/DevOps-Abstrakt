<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>If_Auto_Approved_Cant_Change</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(Status__c),&apos;AUTO-APPROVED&apos;),ISCHANGED(Status__c))</errorConditionFormula>
    <errorMessage>Can&apos;t Change Approval after Auto-Approved</errorMessage>
</ValidationRule>
