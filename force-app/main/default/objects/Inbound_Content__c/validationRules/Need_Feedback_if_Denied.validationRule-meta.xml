<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Need_Feedback_if_Denied</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(Status__c , &quot;Client Denied&quot;), ISBLANK( Feedback__c ) )</errorConditionFormula>
    <errorMessage>Must provide feedback if content is denied</errorMessage>
</ValidationRule>
