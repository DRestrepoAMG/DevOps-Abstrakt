<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Confirmed_Next_Meeting__c</fullName>
    <description>Used for to score how many time the manager marked yes on the field Confirm Next Meeting Date on Partner Meeting Scorecards.</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL( Confirm_Next_Meeting_Date__c ,&apos;Yes&apos;),TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Confirmed Next Meeting</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
