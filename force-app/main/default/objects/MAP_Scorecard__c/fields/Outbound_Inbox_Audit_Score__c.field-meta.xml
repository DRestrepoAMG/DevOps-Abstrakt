<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Outbound_Inbox_Audit_Score__c</fullName>
    <description>Used on Outbound Inbox Audit</description>
    <externalId>false</externalId>
    <formula>(CASE(  Was_every_email_responded_to__c ,
&quot;Yes&quot;,2,
&quot;No&quot;,0,
0)+
CASE(  Email_threads_are_responded_to_Correctly__c,
&quot;Yes&quot;,2,
&quot;No&quot;,0,
0)+
CASE(  Email_threads_put_in_the_correct_folder__c  ,
&quot;Yes&quot;,2,
&quot;No&quot;,0,
0)+
CASE(  Is_the_tech_setup_of_the_inbox_correct__c ,
&quot;Yes&quot;,2,
&quot;No&quot;,0,
0))/8</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Outbound Inbox Audit Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
