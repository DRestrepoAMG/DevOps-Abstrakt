<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cant_Mark_As_Won_Until_Sheet_is_Done</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Client_Setup_Sheet_Completed__c = FALSE,
 ISPICKVAL(StageName,&quot;Won&quot;),RecordTypeId &lt;&gt; &quot;0124A000001gz3W&quot;,RecordTypeId &lt;&gt; &quot;0124A000001gz6B&quot;)</errorConditionFormula>
    <errorMessage>Need to complete Client Setup Sheet before marking opportunity as won</errorMessage>
</ValidationRule>
