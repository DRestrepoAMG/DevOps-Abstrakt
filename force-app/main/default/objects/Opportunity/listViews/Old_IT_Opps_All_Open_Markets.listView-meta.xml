<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Old_IT_Opps_All_Open_Markets</fullName>
    <columns>Contact__c</columns>
    <columns>OPPORTUNITY.NAME</columns>
    <columns>OPPORTUNITY.STAGE_NAME</columns>
    <columns>Follow_up_Date__c</columns>
    <columns>OPPORTUNITY.LAST_ACTIVITY</columns>
    <columns>Metro_Area__c</columns>
    <columns>Reason_Lost__c</columns>
    <columns>Number_of_Available_Programs__c</columns>
    <columns>OPPORTUNITY.LEAD_SOURCE</columns>
    <columns>Manager_Top_5__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OPPORTUNITY.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Opportunity.Recurring_Revenue_Opportunity</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>notEqual</operation>
        <value>Won</value>
    </filters>
    <filters>
        <field>Industry__c</field>
        <operation>equals</operation>
        <value>IT</value>
    </filters>
    <filters>
        <field>Reason_Lost__c</field>
        <operation>equals</operation>
        <value>Canceled - Needs to Reschedule,Closed Market,Ghosting,No Show,Nurture,Proposal - Ghost,Waiting List</value>
    </filters>
    <filters>
        <field>Number_of_Available_Programs__c</field>
        <operation>greaterOrEqual</operation>
        <value>0.5</value>
    </filters>
    <label>Old IT Opps - All Open Markets</label>
    <sharedTo>
        <group>Sales</group>
    </sharedTo>
</ListView>
