<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sales_Closed_Market_View</fullName>
    <booleanFilter>(3 OR (1 AND 2)) AND 4</booleanFilter>
    <columns>OPPORTUNITY.NAME</columns>
    <columns>Contact__c</columns>
    <columns>Metro_Area__c</columns>
    <columns>Industry__c</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>X2nd_Voice__c</columns>
    <columns>OPPORTUNITY.LEAD_SOURCE</columns>
    <columns>OPPORTUNITY.STAGE_NAME</columns>
    <columns>Reason_Lost__c</columns>
    <columns>Most_Recent_Pitch_Date__c</columns>
    <columns>Number_of_Available_Programs__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Delayed,Lost</value>
    </filters>
    <filters>
        <field>Reason_Lost__c</field>
        <operation>contains</operation>
        <value>Closed Market,Waiting List</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>equals</operation>
        <value>Market Closed</value>
    </filters>
    <filters>
        <field>Number_of_Available_Programs__c</field>
        <operation>lessOrEqual</operation>
        <value>0</value>
    </filters>
    <label>Sales - Closed Market View</label>
    <sharedTo>
        <group>Sales</group>
    </sharedTo>
</ListView>
