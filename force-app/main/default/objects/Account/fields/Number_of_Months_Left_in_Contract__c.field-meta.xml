<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Number_of_Months_Left_in_Contract__c</fullName>
    <description>The addition of 1 is for this to be used in the current month</description>
    <externalId>false</externalId>
    <formula>(MONTH( Contract_End_Date__c ) + (12*(YEAR(Contract_End_Date__c)-YEAR(TODAY()))) - MONTH(TODAY()))+1</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Number of Months Left in Contract</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
