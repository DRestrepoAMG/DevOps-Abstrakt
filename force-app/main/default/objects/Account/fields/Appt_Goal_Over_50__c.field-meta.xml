<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Appt_Goal_Over_50__c</fullName>
    <description>Checks the box when percent to monthly appointment goal is greater than 50%</description>
    <externalId>false</externalId>
    <formula>IF( to_Appt_Goal_Actual__c &gt;=(50/100),TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Appt Goal 50% and Over</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
