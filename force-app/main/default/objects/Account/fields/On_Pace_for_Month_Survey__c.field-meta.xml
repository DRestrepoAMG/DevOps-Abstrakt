<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>On_Pace_for_Month_Survey__c</fullName>
    <description>Checks whether surveys are on pace with the percentage of the month complete.</description>
    <externalId>false</externalId>
    <formula>IF( 
OR(Override_Pacing_Metrics__c = True,
(Surveys_Completed_Current_Month__c /(Program_Size__c * 25)  &gt;=   Percent_of_Month_Complete__c )) ,True,False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>On Pace for Month - Survey</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
