<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Appts_On_Pace_for_the_Month__c</fullName>
    <description>Track if appointments are on pace with the percentage of the current month complete.</description>
    <externalId>false</externalId>
    <formula>IF( OR(Override_Pacing_Metrics__c = True,to_Appt_Goal_Actual__c  &gt;=  Percent_of_Month_Complete__c) ,True,False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Appts On Pace for the Month</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
