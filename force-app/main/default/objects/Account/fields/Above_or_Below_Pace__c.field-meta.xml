<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Above_or_Below_Pace__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Percent_of_Contract_Complete__c &gt; Percent_to_Annual_Appt_Goal__c, &quot;Below&quot;,
  IF(Percent_of_Contract_Complete__c &lt;= Percent_to_Annual_Appt_Goal__c, &quot;Above&quot;, &quot;&quot;))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Shows whether we are on pace or behind for client appointments</inlineHelpText>
    <label>Above or Below Pace</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
