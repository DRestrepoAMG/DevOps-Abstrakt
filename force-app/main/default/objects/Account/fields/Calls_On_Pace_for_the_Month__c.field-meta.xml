<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Calls_On_Pace_for_the_Month__c</fullName>
    <description>Checks whether dials are on pace with percentage of the month complete.</description>
    <externalId>false</externalId>
    <formula>IF(  OR(Dial_to_Goal__c  &gt;= Percent_of_Month_Complete__c,  Override_Pacing_Metrics__c = True) ,True,False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Calls On Pace for the Month</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
