<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Must_have_implementation_explanation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
	RecordType.Name &lt;&gt; &apos;Sapper_CSS&apos;,
	ISPICKVAL(Type_of_Implementation__c , &quot;Partial Implementation&quot;),
	ISBLANK(Implementation_Explanation__c
	)
)</errorConditionFormula>
    <errorMessage>Need Explanation for Partial or No Implementation</errorMessage>
</ValidationRule>
