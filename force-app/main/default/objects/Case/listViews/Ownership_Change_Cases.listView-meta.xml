<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ownership_Change_Cases</fullName>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>CASES.STATUS</columns>
    <columns>OWNER_NAME</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>Associated_Action__c</columns>
    <columns>Case_Category__c</columns>
    <columns>Submitted_By__c</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>Expected_Due_Date__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.STATUS</field>
        <operation>equals</operation>
        <value>Open,In Discovery,Being Addressed,On Hold,Waiting on Internal,Waiting on Client,Closed</value>
    </filters>
    <filters>
        <field>CASES.SUBJECT</field>
        <operation>contains</operation>
        <value>Ownership Change</value>
    </filters>
    <filters>
        <field>Case_Category__c</field>
        <operation>notEqual</operation>
        <value>Sales Allocations / Ownership</value>
    </filters>
    <label>Ownership Change Cases</label>
    <sharedTo>
        <allInternalUsers></allInternalUsers>
    </sharedTo>
</ListView>
