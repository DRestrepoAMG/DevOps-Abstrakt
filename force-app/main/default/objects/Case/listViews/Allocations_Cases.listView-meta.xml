<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Allocations_Cases</fullName>
    <booleanFilter>(1 AND 2 AND 3 AND 5) OR (1 AND 3 AND 4 AND 5)</booleanFilter>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>Submitted_By__c</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>Associated_Action__c</columns>
    <columns>CASES.PRIORITY</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.STATUS</field>
        <operation>equals</operation>
        <value>Open,On Hold,In Discovery,Being Addressed,Waiting on Internal,Waiting on Client</value>
    </filters>
    <filters>
        <field>CASES.SUBJECT</field>
        <operation>contains</operation>
        <value>ownership</value>
    </filters>
    <filters>
        <field>OWNER_NAME</field>
        <operation>contains</operation>
        <value>paul h,sean b,danielle b,melissa m,michaela w</value>
    </filters>
    <filters>
        <field>Case_Category__c</field>
        <operation>equals</operation>
        <value>Allocations</value>
    </filters>
    <filters>
        <field>CASES.SUBJECT</field>
        <operation>notContain</operation>
        <value>abstrakt termination - project management</value>
    </filters>
    <label>Allocations Cases</label>
    <sharedTo>
        <allInternalUsers></allInternalUsers>
    </sharedTo>
</ListView>
